{
  "version": 2,
  "builds": [
    {
      "src": "zero-cost-website/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "zero-cost-website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/checkout/store-form-data",
      "dest": "/zero-cost-website/api/store-form-data.js"
    },
    {
      "src": "/api/checkout/direct",
      "dest": "/zero-cost-website/api/checkout-direct.js"
    },
    {
      "src": "/api/checkout/redirect",
      "dest": "/zero-cost-website/api/checkout-redirect.js"
    },
    {
      "src": "/api/checkout/success",
      "dest": "/zero-cost-website/api/process-payment-success.js"
    },
    {
      "src": "/api/process-payment-success",
      "dest": "/zero-cost-website/api/process-payment-success.js"
    },
    {
      "src": "/api/create-payment-intent",
      "dest": "/zero-cost-website/api/create-payment-intent.js"
    },
    {
      "src": "/api/store-form-data",
      "dest": "/zero-cost-website/api/store-form-data.js"
    },
    {
      "src": "/api/supabase-check",
      "dest": "/zero-cost-website/api/supabase-check.js"
    },
    {
      "src": "/api/supabase-test",
      "dest": "/zero-cost-website/api/supabase-test.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/zero-cost-website/api/webhook.js"
    },
    {
      "src": "/api/hello",
      "dest": "/zero-cost-website/api/hello.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/zero-cost-website/api/$1.js"
    },
    {
      "src": "/success",
      "dest": "/zero-cost-website/dist/index.html"
    },
    {
      "src": "/payment-success",
      "dest": "/zero-cost-website/dist/index.html"
    },
    {
      "src": "/payment-test",
      "dest": "/zero-cost-website/public/payment-test.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/zero-cost-website/dist/index.html",
      "status": 200
    }
  ]
}